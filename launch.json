    "version": "0.2.0",
    "configurations": [
        {
            "name": "odoo17 CE",
            "type": "debugpy",
            "request": "launch",
            "python": "${workspaceFolder}/odoo17-venv/bin/python",
            "program": "${workspaceFolder}/odoo/odoo-bin",
            "args": [
                "-c", "${workspaceFolder}/odoo.conf"
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONDEBUG": "1",
                "PYTHONPYCACHEPREFIX": "/tmp/pycache",
                "PYTHONDONTWRITEBYTECODE", "1",
                "GEVENT_SUPPORT": "True",
                "PATH": "/home/desarrollo4/.local/bin:/home/desarrollo4/Apps/python/Python-3.11.9:/home/desarrollo4/Downloads/apache-maven-3.9.6-bin/apache-maven-3.9.6/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games:/usr/local/games:/snap/bin:/snap/bin:/usr/local/bin"
            }
        },
        {
            "name": "odoo17 EE",
            "type": "debugpy",
            "request": "launch",
            "python": "${workspaceFolder}/odoo17-venv/bin/python",
            "program": "${workspaceFolder}/odoo/odoo-bin",
            "args": ["-c", "${workspaceFolder}/odoo_ee.conf"],
            "console": "integratedTerminal",
            "env": {
                "PYTHONDEBUG": "1",
                "PYTHONPYCACHEPREFIX": "/tmp/pycache",
                "PYTHONDONTWRITEBYTECODE", "1",
                "GEVENT_SUPPORT": "True",
                "PATH": "/home/desarrollo4/.local/bin:/home/desarrollo4/Apps/python/Python-3.11.9:/home/desarrollo4/Downloads/apache-maven-3.9.6-bin/apache-maven-3.9.6/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games:/usr/local/games:/snap/bin:/snap/bin:/usr/local/bin"
            }
        },
        {
            "name": "Odoo 16 CE Docker (remote debug)",
            "type": "debugpy",
            "request": "attach",
            "connect": {
                "host": "127.0.0.1",
                "port": 5678
            },
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "."
                }
            ]
        }
    ]
}
